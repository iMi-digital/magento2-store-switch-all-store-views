{
    "name": "imi/magento2-store-switch-all-store-views",
    "description": "This module makes all store views available in the store switcher, regardless of their store or website.",
    "require": {
        "php": "~7.2.0",
        "magento/module-cms": "*",
        "magento/module-store": "*"
    },
    "type": "magento2-module",
    "version": "0.1.0",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "IMI\\StoreSwitch\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "IMI/StoreSwitch"
            ]
        ]
    }
}